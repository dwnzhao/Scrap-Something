<!DOCTYPE html>
<html>
<head>
	<title>Scrap Something | <%= @page_title || 'Collections' %></title>
	<%= stylesheet_link_tag('application', 'collection', 'scrap', 'jquery.fancybox', 'visualize', 'vendor', :media => "all" )%>
	<link rel="stylesheet" href="http://code.jquery.com/ui/1.9.2/themes/base/jquery-ui.css" />

	<%= javascript_include_tag('application') %>	

	<script src="http://code.jquery.com/ui/1.9.2/jquery-ui.js"></script>
	<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.0.2/angular.min.js"></script>
	<script src="http://angular-ui.github.com/angular-ui/build/angular-ui.js"></script>	

	<%= javascript_include_tag('jquery.fancybox.pack', 'collection', 'scrap', 'tab_angular_controller', 'visualize')%>

	<%= csrf_meta_tags %>

</head>
<body>
	<%= render "common/general_menu" %> <br><br>

	<% if !flash[:warning].blank?%>
	<div class="alert">
		<button type="button" class="close" data-dismiss="alert">×</button>
		<%= flash[:warning]%>
	</div>
	<%end%>

	<% if !flash[:notice].blank?%>
	<div class="alert alert-success">
		<button type="button" class="close" data-dismiss="alert">×</button>
		<%= flash[:notice]%>
	</div>
	<%end%>

	<%= yield %>




</body>
</html>